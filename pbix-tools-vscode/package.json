{
    "name": "vscode-pbixtools",
    "displayName": "PBIX Tools",
    "description": "Tools for Power BI Developers",
    "version": "0.4.0",
    "preview": true,
    "author": {
        "name": "Mathias Thierbach"
    },
    "publisher": "mthierba",
    "repository": {
        "type": "git",
        "url": "https://github.com/pbix-tools/pbix-tools.git"
    },
    "engines": {
        "vscode": "^1.21.0"
    },
    "os": [
        "win32"
    ],
    "cpu": [
        "x64"
    ],
    "categories": [
        "Other"
    ],
    "keywords": [
        "Power BI",
        "pbix",
        "pbit"
    ],
    "activationEvents": [
        "workspaceContains:**/.pbixproj.json",
        "workspaceContains:**/*.pbi*",
        "onLanguage:pbix"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "pbixTools.extractPbixHere",
                "title": "Extract PBIX/PBIT here..."
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "when": "resourceExtname =~ /\\.pbi(x|t)/ ",
                    "command": "pbixTools.extractPbixHere",
                    "group": "0_pbixtools"
                }
            ]
        },
        "configuration":[
        ],
        "languages": [{
            "id": "pbix",
            "extensions": [
                ".pbix",
                ".pbit"
            ]
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
